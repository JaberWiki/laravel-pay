{
    "name": "jaberwiki/laravel-pay",
    "description": "A Laravel Package for Paystack",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "payments",
        "subscription",
        "paystack",
        "paystack.co"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Jaber",
            "email": "jaberphpdev@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.8.15|^6.0|^7.0",
        "guzzlehttp/guzzle": "5.*|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "scrutinizer/ocular": "~1.1",
        "php-coveralls/php-coveralls": "^2.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "jaberwiki\\Paystack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jaberwiki\\Paystack\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "jaberwiki\\Paystack\\PaystackServiceProvider"
            ],
            "aliases": {
                "Paystack": "jaberwiki\\Paystack\\Facades\\Paystack"
            }
        }
    }
}
